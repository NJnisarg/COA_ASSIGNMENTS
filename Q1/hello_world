#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 9;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1e43760 .scope module, "test" "test" 2 12;
 .timescale -9 -9;
v0x1e86070_0 .var "clock", 0 0;
S_0x1e44070 .scope module, "hello" "hello_world" 2 15, 3 10, S_0x1e43760;
 .timescale 0 0;
v0x1e44160_0 .net "clock", 0 0, v0x1e86070_0; 1 drivers
E_0x1e43690 .event edge, v0x1e44160_0;
    .scope S_0x1e44070;
T_0 ;
    %wait E_0x1e43690;
    %vpi_call 3 16 "$monitor", "Hello world %0t\012", $realtime;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x1e43760;
T_1 ;
    %vpi_call 2 19 "$dumpfile", "hello_world.vcd";
    %vpi_call 2 20 "$dumpvars", 1'sb0, S_0x1e43760;
    %end;
    .thread T_1;
    .scope S_0x1e43760;
T_2 ;
    %set/v v0x1e86070_0, 1, 1;
    %movi 8, 99, 8;
T_2.0 %cmp/s 0, 8, 8;
    %jmp/0xz T_2.1, 5;
    %add 8, 1, 8;
    %delay 1, 0;
    %load/v 16, v0x1e86070_0, 1;
    %inv 16, 1;
    %set/v v0x1e86070_0, 16, 1;
    %jmp T_2.0;
T_2.1 ;
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test_hello_world.v";
    "hello_world.v";
